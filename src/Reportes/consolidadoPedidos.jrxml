	<queryString>
		<![CDATA[SELECT dc.descripcion_corta tipo_documento,
    dp.numero_documento,
    dp.apellido_paterno,
    dp.apellido_materno,
    dp.nombres,
    dc2.descripcion_corta localidad,
    dp.centro_costo,
    dc3.descripcion_larga tipo_servicio,
    dp.costo_servicio,
    date_format(dp.fecha_creacion,'%d/%m/%Y') fecha,
    dp.centro_costo,
    us.username
FROM detalle_pedido dp inner join pedido pe on pe.id_pedido = dp.id_pedido
inner join usuario us on us.id_usuario = pe.id_usuario_solicita
inner join organizacion org on org.id_organizacion = pe.id_organizacion
inner join detalle_catalogo dc on dc.id_catalogo =  1 and dc.codigo = dp.id_tipo_documento
inner join detalle_catalogo dc2 on dc2.id_catalogo =  8 and dc2.codigo = dp.id_localidad
inner join detalle_catalogo dc3 on dc3.id_catalogo =  6 and dc3.codigo = dp.id_tipo_servicio
where org.id_organizacion =$P{P_ID_ORGANIZACION} and dp.id_tipo_servicio = ifnull($P{P_ID_SERVICIO},dp.id_tipo_servicio) and  STR_TO_DATE(DATE_FORMAT(pe.fecha_creacion,'%d/%m/%Y'), '%d/%m/%Y')  between $P{P_FECHA_DESDE} and $P{P_FECHA_HASTA} and dp.id_estado not in (3) order by dp.id_localidad, dp.id_tipo_servicio]]>
	</queryString>
